{
  "name": "products-backend",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "build": "pnpm exec babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files --no-copy-ignored",
    "start": "cross-env NODE_ENV=development node dist/src/shared/infra/http/server.js",
    "dev": "cross-env NODE_ENV=staging ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules ./src/shared/infra/http/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "prepare": "husky"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "0.3.20",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.11.10",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.5.2"
  }
